{
  "name": "brightecapital/brighte-sdk",
  "description": "Brighte SDK",
  "keywords": [
    "Brighte",
    "SDK"
  ],
  "homepage": "https://github.com/brightecapital/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ken.ngo@brighte.com.au"
    }
  ],
  "require": {
    "php": "^7.1",
    "enqueue/sqs": "^0.10",
    "enqueue/sns": "^0.10",
    "aws/aws-sdk-php": "3.171",
    "brightecapital/service-schema": "^1.2",
    "doctrine/orm": "^2.5",
    "predis/predis": "^1.1",
    "guzzlehttp/guzzle": "^6.3",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Brighte\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brighte\\Test\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
